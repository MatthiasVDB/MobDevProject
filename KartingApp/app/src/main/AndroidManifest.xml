<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="be.pxl.kartingapp">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddNewSessionActivity"
            android:label="@string/add_session_activity">
        </activity>
        <activity
            android:name=".DrawLineChartActivity"
            android:label="@string/draw_line_chart_activity">
        </activity>
        <activity
            android:name=".SessionsActivity"
            android:label="@string/sessions_activity">
        </activity>
        <activity
            android:name=".LapsActivity"
            android:label="@string/laps_activity">
        </activity>
    </application>

</manifest>